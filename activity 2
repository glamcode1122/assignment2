# Base class
class Vehicle:
    def move(self):
        raise NotImplementedError("This method should be overridden by subclasses")

# Subclasses with different implementations
class Car(Vehicle):
    def move(self):
        print("Driving on the road ğŸš—")

class Plane(Vehicle):
    def move(self):
        print("Flying in the sky âœˆï¸")

class Boat(Vehicle):
    def move(self):
        print("Sailing on water ğŸš¢")
